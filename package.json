{
  "name": "starter",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev:client": "vite",
    "build": "tsc && vite build",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "test": "jest",
    "prod:server": "node dist/server.js",
    "dev:watchnexec:server": "nodemon --watch 'src/**/*.ts' --exec 'ts-node -r tsconfig-paths/register' src/server.ts",
    "dev:server": "nodemon --exec node --no-warnings=ExperimentalWarning --loader ts-node/esm src/server.ts",
    "clean": "rm -rf dist client/build",
    "prod:client": "vite build",
    "prod": "yarn prod:client && yarn prod:server",
    "dev": "concurrently \"yarn dev:server\" \"yarn dev:client\""
  },
  "dependencies": {
