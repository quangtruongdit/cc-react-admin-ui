    "@reduxjs/toolkit": "^2.2.7",
    "@tanstack/react-query": "^4.29.19",
    "@types/cors": "^2.8.17",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.21.0",
    "express-http-proxy": "^2.1.1",
    "formik": "^2.4.6",
    "jquery": "^3.7.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-error-boundary": "^4.0.11",
    "react-hook-form": "^7.53.0",
